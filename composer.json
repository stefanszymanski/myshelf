{
    "name": "sszymanski/myshelf",
    "description": "Book management CLI tool",
    "keywords": ["laravel", "laravel zero", "console", "cli"],
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Stefan Szymanski",
            "email": "stefan.szymanski@posteo.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel-zero/framework": "^8.8",
        "nicebooks/isbn": "^0.3.9",
        "rakibtg/sleekdb": "^2.13",
        "symfony/string": "^5.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3",
        "phpstan/phpstan": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["myshelf"]
}
